{
    "name": "yalesov/zf2-geoname",
    "description": "maintain a local copy of the Geonames database",
    "keywords": ["geoname", "zf2", "zendframework"],
    "authors": [
        {
            "name": "Yulij Andreevich Lesov",
            "email": "yalesov@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "yalesov/yaml": "1.*",
        "zendframework/zendframework": "2.*",
        "yalesov/arg-validator": "1.*",
        "yalesov/zf2-doctrine": "1.*",
        "yalesov/zf2-cron": "2.*",
        "yalesov/zf2-cli": "1.*",
        "yalesov/file-system-manager": "1.*"
    },
    "require-dev": {
        "yalesov/zf2-phpunit-testcase": "1.*",
        "yalesov/zf2-phing-task": "1.*"
    },
    "autoload": {
        "psr-0": {
            "Yalesov\\Geoname": "src/"
        }
    },
    "license": "ISC",
    "homepage": "https://github.com/yalesov/zf2-geoname",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/yalesov/zf2-geoname"
        }
    ],
    "support": {
        "issues": "https://github.com/yalesov/zf2-geoname/issues"
    }
}
